                selectorElementLanguage = `https://cdn.jsdelivr.net/gh/truong-huong/nullshell-js/prism-${language}.min.js`;
                (!codeArray.includes(selectorElementLanguage)) ? codeArray.push(selectorElementLanguage): '';
                pos = pos + 1;
                addLanguageScript(pos);
            })() : (function() {
                (codeArray.length > 0) ? (function() {
                    var hongtham = document.createElement('script');
                    document.head.appendChild(hongtham);
                    hongtham.onload = function() {
                        (codeArray.length > 0) ? (function() {
                            addLanguageScript((selectorElementsLength + 1))
                        })() : (function() {
                            Prism.highlightAll()
                        })()
                    }
                    hongtham.src = codeArray.shift();
                })() : '';
            })();
        }

