        var addLanguageScript = (pos) => {
            (pos) ? '' : (pos = 0);
            var selectorElements = document.querySelectorAll('code[class*=language-]');
            var selectorElementsLength = selectorElements.length;
            var selectorElement = selectorElements[pos];
            var selectorElementLanguage;
            (selectorElementsLength && pos < selectorElementsLength) ? (function() {
                var language = selectorElement.getAttribute('class').replace('language-', '').trim();
