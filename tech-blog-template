        var expand = (obj) => {
            var keys = Object.keys(obj);
            for (let i = 0; i < keys.length; ++i) {
                let key = keys[i],
                    subkeys = key.split(/,\s?/),
                    target = obj[key];
                delete obj[key];
                subkeys.forEach(key => obj[key] = target)
            }
            return obj;
        }
