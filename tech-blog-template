        var codeArray = ['https://cdn.jsdelivr.net/gh/truong-huong/nullshell-js/main.js'];
        var formatPreCode = (a) => {
            (document.querySelectorAll('pre > code').length > 0) ? (function() {
                (a > 0) ? "" : (a = 0);
                var b = document.querySelectorAll('pre > code')[a];
                var c = b.innerHTML;
                var d = c.replace(/(\<br(.*?)\>)/gim, '&#10;');
                b.innerHTML = d;
                a = a + 1;
                (a < document.querySelectorAll('pre > code').length) ? (formatPreCode(a)) : "";
            })() : '';
