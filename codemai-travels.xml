	.introduce-meta {
		margin-left: 100px;
	}
	.introduce-image {
		position: relative;
		float: right;
		width: 100%;
	}
	.introduce-image:after {
		content: "";
		display: block;
		padding-bottom: 100%;
	}
	.introduce-image img {
		max-width: 300px;
		max-height: 300px;
		position: absolute;
		top: 0;
		bottom: 0;
		left: 0;
		right: 0;
		width: 100%;
		height: 100%;
		object-fit: cover;
		object-position: center;
	}
	.introduce-tag {
		position: absolute;
		display: flex;
		flex-direction: column;
		top: 20%;
		margin-left: -100px;
		padding: 20px;
		color: #000;
		background-color: #fff;
		border: solid 1px;
		opacity: 0.9;
